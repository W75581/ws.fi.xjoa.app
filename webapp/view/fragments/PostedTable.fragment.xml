                    <Column>
                        <Text text="FI Document Number" />
                    </Column>
                    <Column>
                        <Text text="Sender JV" />
                    </Column>
                    <Column>
                        <Text text="Sender Equity Type" />
                    </Column>
                    <Column>
                        <Text text="Sender Cost Center" />
                    </Column>
                    <Column>
                        <Text text="Sender WBS Element" />
                    </Column>
                    <Column>
                        <Text text="Sender Cost Element" />
                    </Column>
                    <Column>
                        <Text text="Sender Activity" />
                    </Column>
                    <Column>
                        <Text text="Receiver Country" />
                    </Column>
                    <Column>
                        <Text text="Receiver Company Code" />
                    </Column>
                    <Column>
                        <Text text="Receiver CC FI Document Number" />
                    </Column>
                    <Column>
                        <Text text="Receiver CC Fiscal Year" />
                    </Column>
                    <Column>
                        <Text text="Receiver JV" />
                    </Column>
                    <Column>
                        <Text text="Receiver Equity Type" />
                    </Column>
                    <Column>
                        <Text text="Receiver Cost Center" />
                    </Column>
                    <Column>
                        <Text text="Receiver WBS Element" />
                    </Column>
                    <Column>
                        <Text text="Receiver Cost Element" />
                    </Column>
                    <Column>
                        <Text text="Receiver Activity" />
                    </Column>
                    <Column>
                        <Text text="Agreement Type" />
                    </Column>
                    <Column>
                        <Text text="Currency" />
                    </Column>
                    <Column>
                        <Text text="Transaction Currency" />
                    </Column>
                    <Column>
                        <Text text="Amount In Transaction Currency" />
                    </Column>
                    <Column>
                        <Text text="Company Code Currency" />
                    </Column>
                    <Column>
                        <Text text="Amount In Company Code Currency" />
                    </Column>
                    <Column>
                        <Text text="GlobalCurrency" />
                    </Column>
                    <Column>
                        <Text text="Amount In Global Currency" />
                    </Column>
                    <Column>
                        <Text text="Customer" />
                    </Column>
                    <Column>
                        <Text text="Output Tax Code" />
                    </Column>
                    <Column>
                        <Text text="Sender ICO Adj GL" />
                    </Column>
                    <Column>
                        <Text text="Vendor" />
                    </Column>
                    <Column>
                        <Text text="Input Tax Code" />
                    </Column>
                    <Column>
                        <Text text="WHT" />
                    </Column>
                    <Column>
                        <Text text="Receiver ICO Adj GL" />
                    </Column>
                    <Column>
                        <Text text="SummaryInvoiceIndicator" />
                    </Column>
                    <Column>
                        <Text text="Markup Percentage" />
                    </Column>
                    <Column>
                        <Text text="Markup GL" />
                    </Column>
                    <Column>
                        <Text text="Posted Document Number SCC" />
                    </Column>
                    <Column>
                        <Text text="Posted Document FY SCC" />
                    </Column>
                    <Column>
                        <Text text="Posted Document Number RCC" />
                    </Column>
                    <Column>
                        <Text text="Posted Document FY RCC" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem vAlign="Middle">
                        <cells>
                            <Text text="{mdlForm>Period}" />
                            <Text text="{mdlForm>FiscalYear}" />
                            <Text text="{mdlForm>Message}" />
                            <Text text="{mdlForm>DocumentDate}" />
