<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartTable="sap.ui.comp.smarttable">
    <Dialog title="Posted Records" stretch="true">
        <content>
            <Table id="idTablePostedRecs" autoPopinMode="true" items="{mdlForm>/postRec}" sticky="ColumnHeaders" popinLayout="GridSmall" noDataText="" alternateRowColors="true" selectionChange="handleItemSelection" rememberSelections="false">
