                            <Text text="{mdlForm>PostedDocumentNumberSCC}" />
                            <Text text="{mdlForm>PostedDocumentFYSCC}" />
                            <Text text="{mdlForm>PostedDocumentNumberRCC}" />
                            <Text text="{mdlForm>PostedDocumentFYRCC}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
        <endButton>
            <Button text="Close" icon="sap-icon://cancel" type="Reject" press="onPostTblDiagClose" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
